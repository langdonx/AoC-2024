<Query Kind="Statements" />

// build a matrix [y][x]
var matrix = Input.Puzzle
    .Split(Environment.NewLine)
    .Select(line => line.ToCharArray())
    .ToArray();

//Input.Sample.Dump();

var xmasCount = 0;

for (var y = 0; y < matrix.Length; y++)
{
    for (var x = 0; x < matrix[y].Length; x++)
    {
        if (matrix[y][x] == 'X')
        {
            // check horizontal
            xmasCount += (matrix.GetHorizontalWord(y, x) == "XMAS") ? 1 : 0;
            // check horizontal right
            xmasCount += (matrix.GetHorizontalWord(y, x, backward: true) == "XMAS") ? 1 : 0;

            // check vertical up
            xmasCount += (matrix.GetVerticalWord(y, x) == "XMAS") ? 1 : 0;
            // check vertical down
            xmasCount += (matrix.GetVerticalWord(y, x, backward: true) == "XMAS") ? 1 : 0;

            // check diagonal nw-se
            xmasCount += (matrix.GetDiagonalWord(y, x) == "XMAS") ? 1 : 0;
            // check diagonal ne-sw
            xmasCount += (matrix.GetDiagonalWord(y, x, horizontalBackward: true) == "XMAS") ? 1 : 0;
            // check diagonal se-nw
            xmasCount += (matrix.GetDiagonalWord(y, x, verticalBackward: true, horizontalBackward: true) == "XMAS") ? 1 : 0;
            // check diagonal sw-ne
            xmasCount += (matrix.GetDiagonalWord(y, x, verticalBackward: true) == "XMAS") ? 1 : 0;
        }
    }
}

Console.WriteLine(xmasCount);

public static class ExtensionMethods
{
    public static string GetHorizontalWord(this char[][] matrix, int y, int x, bool backward = false)
    {
        var multiplier = backward ? -1 : 1;
        var result = "";

        // out of range errors be damned
        try
        {
            result = $"{matrix[y][x]}{matrix[y][x + 1 * multiplier]}{matrix[y][x + 2 * multiplier]}{matrix[y][x + 3 * multiplier]}";
        }
        catch { }

        return result;
    }

    public static string GetVerticalWord(this char[][] matrix, int y, int x, bool backward = false)
    {
        var multiplier = backward ? -1 : 1;
        var result = "";

        // out of range errors be damned
        try
        {
            result = $"{matrix[y][x]}{matrix[y + 1 * multiplier][x]}{matrix[y + 2 * multiplier][x]}{matrix[y + 3 * multiplier][x]}";
        }
        catch { }

        return result;
    }

    public static string GetDiagonalWord(this char[][] matrix, int y, int x, bool verticalBackward = false, bool horizontalBackward = false)
    {
        var verticalMultiplier = verticalBackward ? -1 : 1;
        var horizontalMultiplier = horizontalBackward ? -1 : 1;
        var result = "";

        // out of range errors be damned
        try
        {
            result = $"{matrix[y][x]}{matrix[y + 1 * verticalMultiplier][x + 1 * horizontalMultiplier]}{matrix[y + 2 * verticalMultiplier][x + 2 * horizontalMultiplier]}{matrix[y + 3 * verticalMultiplier][x + 3 * horizontalMultiplier]}";
        }
        catch { }

        return result;
    }
}

