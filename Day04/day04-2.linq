<Query Kind="Statements" />

// build a matrix [y][x]
var matrix = Input.Puzzle
    .Split(Environment.NewLine)
    .Select(line => line.ToCharArray())
    .ToArray();

var validWords = new string[] { "MAS", "SAM" };
var xMasCount = 0;

for (var y = 0; y < matrix.Length; y++)
{
    for (var x = 0; x < matrix[y].Length; x++)
    {
        if (matrix[y][x] == 'A')
        {
            // check diagonal nw-se
            var word1 = matrix.GetDiagonal(y, x, "nwse");
            var word2 = matrix.GetDiagonal(y, x, "nesw");

            xMasCount += (validWords.Contains(word1) && validWords.Contains(word2)) ? 1 : 0;
        }
    }
}

Console.WriteLine(xMasCount);

public static class ExtensionMethods
{
    public static string GetDiagonal(this char[][] matrix, int y, int x, string direction)
    {
        // out of range errors be damned
        try
        {
            switch (direction)
            {
                case "nwse":
                    return $"{matrix[y - 1][x - 1]}{matrix[y][x]}{matrix[y + 1][x + 1]}";
                case "nesw":
                    return $"{matrix[y + 1][x - 1]}{matrix[y][x]}{matrix[y - 1][x + 1]}";
            }
        }
        catch { }

        return "";
    }
}

